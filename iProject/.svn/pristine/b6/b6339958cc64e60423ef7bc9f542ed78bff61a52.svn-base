* { padding:0; margin:0;  font-size:12px;}
h1, h2, h3, h4, h5, h6, input, select, div, form { padding:0; margin:0; }
img { border:none; }
fieldset { border:0 none; margin:0; padding:0; }
dl, ul, ol, li { list-style: none; }
hr { display:none; }

p{ line-height:24px; text-align:justify;}

caption, legend { position: absolute; width: 0; height: 0; font-size: 0; line-height: 0; text-indent: -9999em; }
.displaynone { position: absolute; width: 0; height: 0; font-size: 0; line-height: 0; text-indent: -9999em; }
.clearboth { clear:both; height:0; line-height:0; }
* html .clearboth { display:inline; height:0; line-height:0; }
a:link, a:visited {  color:#636162; text-decoration: none; }
a:hover, a:focus, a:active {  color:#636162; text-decoration:none; }

body { line-height:1.5em; font-family: 'Nanum Gothic', sans-serif; font-size:12px;  }
#wrap { width:970px; margin:0 auto; }

/* header */
#wrap #header {position:relative; width:970px; height:120px; padding:5px; display:block; }

/* gnb */
#wrap #header .gnb {position:absolute; top:10px; right:50px;;}
#wrap #header .gnb li {display:inline-block; float: left; margin-left:10px; background:  padding-left:10px;}
#wrap #header .gnb li:first-child {background:none; padding-left:0; margin-left:0;}
#wrap #header .gnb li a {color:#979797; font-weight:bold; letter-spacing:-1px;}
#wrap #header .top_tit { position:absolute; top:42px; left:400px;}


/* lnb */
#lnb {position:absolute; top:40px; display:block;   width:920px; height:28px; padding:5px;}
#lnb li {display:inline;  margin-left:5px; padding-left:65px;}
#lnb .sun_lnb {position:absolute; top:48px; left:30px; height:24px; width:auto;  background:url(../images/top/nav_sub_bg.png);}
#lnb .sun_lnb li {padding-left:5px;}
#lnb .sun_lnb a {color:#fff; font-size:11px; margin-right:10px; font-weight:bold;}
#lnb .sun_lnb a:hover, #lnb .sun_lnb a:focus, #lnb .sun_lnb a:active, #lnb .sun_lnb .current {font-weight:bold;}

#container {width:100%; display:inline-block; margin-top:13px;}

