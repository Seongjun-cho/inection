#footer .f_list { width:720px; display:inline-block; margin-bottom:15px; }
#footer .f_list li {float:left; display:inline-block; padding:4px 0 0 15px;}
#footer .f_list li a {color:#ffffff !important; font-weight:bold; font-size:12px; letter-spacing:1px;}
#footer address p{ color:#666; font-style:normal; font-weight:bold; font-size:11px !important; letter-spacing:0px;  text-align:left;}



/* skipToContent */
#skipToContent { position:absolute; width:100%; z-index:999; left:0; }
#skipToContent a { display:block; height:1px; width:1px; margin-bottom:-1px; overflow:hidden; text-align:center; color:#fff; white-space:nowrap; background:#000; }
#skipToContent a:focus, #skipToContent a:active { height:auto; width:auto; padding:5px; margin-bottom:10px; }

